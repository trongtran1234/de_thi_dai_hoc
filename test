`ifndef _ARBITER_SCOREBOARD_
`define _ARBITER_SCOREBOARD_

class arbiter_scoreboard extends uvm_scoreboard;
    
   `uvm_component_utils(arbiter_scoreboard)
   uvm_analysis_export #(arbiter_package) monitor_before;
   uvm_analysis_export #(arbiter_package) monitor_after;
   arbiter_package trans_before;
   arbiter_package trans_after;
   uvm_tlm_analysis_fifo#(arbiter_package) fifo_before;
   uvm_tlm_analysis_fifo#(arbiter_package) fifo_after;
   
   function new (string name = "", uvm_component parent);
      super.new(name, parent);
      trans_before = new("trans_before");
      trans_after = new("trans_after");
   endfunction: new
   
   function void build_phase(uvm_phase phase);
      super.build_phase(phase);
      fifo_before = new(.name("fifo_before"), .parent(this));
      fifo_after = new(.name("fifo_after"), .parent(this));
      monitor_before = new(.name("monitor_before"), .parent(this));
      monitor_after = new(.name("monitor_after"), .parent(this));
   endfunction: build_phase
   
   function void connect_phase(uvm_phase phase);
      monitor_before.connect(fifo_before.analysis_export);
      monitor_after.connect(fifo_after.analysis_export);
   endfunction: connect_phase

   task run();
      forever begin
         fifo_before.get(trans_before);
         fifo_after.get(trans_after);
         compare();
      end
   endtask: run
   
   virtual function void compare();
    //// Coding for checker
   endfunction: compare

endclass: arbiter_scoreboard

`endif







////////////////////////////////////////////////////
//// Author: trongk10@gmail.com
//// Date  : 05/26/2022
////////////////////////////////////////////////////

`ifndef _ARBITER_PACKAGE_
`define _ARBITER_PACKAGE_

class arbiter_package extends uvm_sequence_item;

   bit  P0_ACK;
   bit  P1_ACK;
   bit  P2_ACK;
   bit  P3_ACK;

   rand bit P0_REQ;
   rand bit P1_REQ;
   rand bit P2_REQ;
   rand bit P3_REQ;

   
   constraint constr_P0 {
      P0_REQ inside {[0:1]};
   }
   
   constraint constr_P1 {
      P0_REQ inside {[0:1]};
   }
   
   constraint constr_P2 {
      P0_REQ inside {[0:1]};
   }

   constraint constr_P3 {
      P0_REQ inside {[0:1]};
   }

   
   function new (string name = "");
      super.new(name);
   endfunction: new
   
   `uvm_object_utils_begin(arbiter_package)
      `uvm_field_int(P0_ACK, UVM_ALL_ON)
      `uvm_field_int(P1_ACK, UVM_ALL_ON)
      `uvm_field_int(P2_ACK, UVM_ALL_ON)
      `uvm_field_int(P3_ACK, UVM_ALL_ON)
      `uvm_field_int(P0_REQ, UVM_ALL_ON)
      `uvm_field_int(P1_REQ, UVM_ALL_ON)
      `uvm_field_int(P2_REQ, UVM_ALL_ON)
      `uvm_field_int(P3_REQ, UVM_ALL_ON)
   `uvm_object_utils_end

endclass: arbiter_package
`endif


`ifndef _ARBITER_SEQUENCE_
`define _ARBITER_SEQUENCE_

class arbiter_sequence extends uvm_sequence #(arbiter_package);
   `uvm_object_utils(arbiter_sequence)
   
   function new (string name = "");
     super.new(name);
   endfunction: new
   
   virtual task pre_body();
      `uvm_info("CHECK_FLAG_PRE_BODY", $sformatf("Optional"), UVM_MEDIUM)
       if (starting_phase != null) starting_phase.raise_objection(this);
   endtask: pre_body
 
    virtual task body();
       arbiter_package add_trans;
          add_trans = arbiter_package::type_id::create(.name("add_trans"), .contxt(get_full_name()));
          `ifdef RANDOM
             `include "TM/tm_uvm_random.v"
          `elsif UVM_CREATE
             `include "TM/tm_uvm_create.v"
          `elsif UVM_DO_WITH
             `include "TM/tm_uvm_do_with.v"
          `else
             repeat (100) begin
             start_item(add_trans);
             assert (add_trans.randomize ());
             `uvm_info("RANDOM_INPUT", $sformatf("P0_REQ: %b, P1_REQ: %b, P2_REQ: %b, P3_REQ: %b", add_trans.P0_REQ, add_trans.P1_REQ, add_trans.P2_REQ, add_trans.P3_REQ), UVM_MEDIUM)
             finish_item(add_trans);
             end
          `endif
   endtask: body
 
 
   virtual task post_body();
      `uvm_info("CHECK_FLAG_POST_BODY", $sformatf("Optional"), UVM_MEDIUM)
      if (starting_phase != null) starting_phase.drop_objection(this);
   endtask:post_body
 
 
 
endclass: arbiter_sequence
`endif

`ifndef _UVM_SEQUENCER_
`define _UVM_SEQUENCER_

typedef uvm_sequencer#(arbiter_package) arbiter_sequencer ;
`endif







