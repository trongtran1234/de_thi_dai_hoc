`include "arbiter_pkg.sv"
`include "arbiter_if.sv"
`include "RTL/arbiter.v"
`include "SVA/sva_test.v"

module arbiter_tb_top;

   import uvm_pkg::*;
   import arbiter_pkg::*;
   arbiter_if vif();
   
   arbiter arbiter_00 (
      vif.ARB_RST,
      vif.ARB_CLK,
      vif.P0_CLK,
      vif.P1_CLK,
      vif.P2_CLK,
      vif.P3_CLK,
      vif.P0_REQ,
      vif.P1_REQ,
      vif.P2_REQ,
      vif.P3_REQ,
      vif.P0_ACK,
      vif.P1_ACK,
      vif.P2_ACK,
      vif.P3_ACK);

   bind arbiter sva sva_00 (
      .ARB_RST(ARB_RST),
      .ARB_CLK(ARB_CLK),
      .P0_CLK(P0_CLK),
      .P1_CLK(P1_CLK),
      .P2_CLK(P2_CLK),
      .P3_CLK(P3_CLK),
      .P0_REQ_sync(P0_REQ_sync),
      .P1_REQ_sync(P1_REQ_sync),
      .P2_REQ_sync(P2_REQ_sync),
      .P3_REQ_sync(P3_REQ_sync),
      .P0_ACK_sync(P0_ACK_sync),
      .P1_ACK_sync(P1_ACK_sync),
      .P2_ACK_sync(P2_ACK_sync),
      .P3_ACK_sync(P3_ACK_sync)
   );   


   initial begin
      uvm_resource_db #(virtual arbiter_if)::set(.scope("ifs"), .name("arbiter_if") , .val(vif));
      run_test();
   end
   

   `include "generate_clk_and_reset.v"
   
   initial begin
      $recordfile("filename.trn");
      $recordvars();
   end

endmodule: arbiter_tb_top







