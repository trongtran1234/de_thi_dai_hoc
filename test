`ifndef _ARBITER_TEST_
`define _ARBITER_TEST_

class arbiter_test extends uvm_test;
   `uvm_component_utils(arbiter_test)
   
   arbiter_env env_trans;
      function new (string name = "", uvm_component parent);
      super.new(name, parent);
   endfunction: new
   
   function void build_phase(uvm_phase phase);
      super.build_phase(phase);
      env_trans = arbiter_env::type_id::create(.name("env_trans"), .parent(this));
   endfunction: build_phase
   
   task run_phase(uvm_phase phase);
      arbiter_sequence sequencer_trans;
      phase.raise_objection(.obj(this));
      sequencer_trans = arbiter_sequence::type_id::create(.name("sequencer_trans"), .contxt(get_full_name()));
      assert(sequencer_trans.randomize());
      `uvm_info("UVM_TEST_LP","START Simulation",UVM_NONE)
      sequencer_trans.start(env_trans.agent_trans.sequencer_trans);
      phase.drop_objection(.obj(this));
   endtask: run_phase

endclass: arbiter_test

`endif









   initial begin
      vif.ARB_CLK = 1;
      vif.P0_CLK  = 1;
      vif.P1_CLK  = 1;
      vif.P2_CLK  = 1;
      vif.P3_CLK  = 1;
      vif.ARB_RST = 1;
      #80;
      vif.ARB_RST = 0;
//      #300;
//      vif.ARB_RST = 1; //// Test for reset SVA
      #20000;
   end

   always #8  vif.ARB_CLK = ~vif.ARB_CLK;
   always #9  vif.P0_CLK  = ~vif.P0_CLK;
   always #10 vif.P1_CLK  = ~vif.P1_CLK;
   always #11 vif.P2_CLK  = ~vif.P2_CLK;
   always #12 vif.P3_CLK  = ~vif.P3_CLK;








