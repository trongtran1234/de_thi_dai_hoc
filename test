////////////////////////////////////////////////////
//// Author: trongk10@gmail.com
//// Date  : 05/26/2022
////////////////////////////////////////////////////

`ifndef _ARBITER_MONITOR_
`define _ARBITER_MONITOR_

class arbiter_monitor_before extends uvm_monitor;
   `uvm_component_utils(arbiter_monitor_before)
   uvm_analysis_port#(arbiter_package) mon_before;
   virtual arbiter_if vif;
   
   function new (string name, uvm_component parent);
      super.new(name, parent);
   endfunction: new
   
   function void build_phase(uvm_phase phase);
      super.build_phase(phase);
      void'(uvm_resource_db#(virtual arbiter_if)::read_by_name(.scope("ifs"), .name("arbiter_if"), .val(vif)));
      mon_before = new(.name("mon_before"), .parent(this));
   endfunction: build_phase
   
   task run_phase(uvm_phase phase);
      integer count = 0;
      arbiter_package trans_mo1;
      trans_mo1 = arbiter_package::type_id::create(.name("trans_mo1"), .contxt(get_full_name()));
      forever begin
          @(posedge vif.P0_CLK) begin
             trans_mo1.P0_ACK = vif.P0_ACK;
//             mon_before.write(trans_mo1);
             `uvm_info("UVM_RESULT_FROM_DUT", $sformatf("P0_ACK: %b", trans_mo1.P0_ACK), UVM_LOW) 
          end
          @(posedge vif.P1_CLK) begin
             trans_mo1.P1_ACK = vif.P1_ACK;
             `uvm_info("UVM_RESULT_FROM_DUT", $sformatf("P1_ACK: %b", trans_mo1.P1_ACK), UVM_LOW)  
          end
          @(posedge vif.P2_CLK) begin
             trans_mo1.P2_ACK = vif.P2_ACK;
             `uvm_info("UVM_RESULT_FROM_DUT", $sformatf("P2_ACK: %b", trans_mo1.P2_ACK), UVM_LOW)
          end
          @(posedge vif.P3_CLK) begin
             trans_mo1.P3_ACK = vif.P3_ACK;
             `uvm_info("UVM_RESULT_FROM_DUT", $sformatf("P3_ACK: %b", trans_mo1.P3_ACK), UVM_LOW)
          end
             mon_before.write(trans_mo1);
       end
   endtask: run_phase

endclass: arbiter_monitor_before



class arbiter_monitor_after extends uvm_monitor;
   `uvm_component_utils(arbiter_monitor_after)
   virtual arbiter_if vif;
   uvm_analysis_port#(arbiter_package) mon_after;
   arbiter_package trans_cv;
   arbiter_package trans_mo2;
    
   covergroup arbiter_cov;
      P0_ACK_cv: coverpoint trans_cv.P0_ACK;
      P1_ACK_cv: coverpoint trans_cv.P1_ACK;
      P2_ACK_cv: coverpoint trans_cv.P2_ACK;
      P3_ACK_cv: coverpoint trans_cv.P3_ACK;
      cross P0_ACK_cv, P1_ACK_cv, P2_ACK_cv, P3_ACK_cv;
   endgroup: arbiter_cov

   function new(string name, uvm_component parent);
      super.new(name, parent);
      arbiter_cov = new;
   endfunction: new
   
   function void build_phase(uvm_phase phase);
      super.build_phase(phase);
      void'(uvm_resource_db#(virtual arbiter_if)::read_by_name(.scope("ifs"), .name("arbiter_if"), .val(vif)));
      mon_after = new(.name("mon_after"), .parent(this));
   endfunction: build_phase
   
   task run_phase(uvm_phase phase);
      integer count = 0;
      integer tmp = 0;
      trans_mo2 = arbiter_package::type_id::create(.name("trans_mo2"), .contxt(get_full_name()));
      forever begin
         @(posedge vif.P0_CLK) begin
            trans_mo2.P0_ACK = 1'b0;
         end
         @(posedge vif.P1_CLK) begin
            trans_mo2.P1_ACK = 1'b0;
         end
         @(posedge vif.P2_CLK) begin
            trans_mo2.P2_ACK = 1'b0;
         end
         @(posedge vif.P3_CLK) begin
            trans_mo2.P3_ACK = 1'b0;
         end
         trans_cv = trans_mo2;
         arbiter_cov.sample();
         mon_after.write(trans_mo2);
      end
   endtask: run_phase
endclass: arbiter_monitor_after

`endif







////////////////////////////////////////////////////
//// Author: trongk10@gmail.com
//// Date  : 05/26/2022
////////////////////////////////////////////////////

`ifndef _ARBITER_ENV_
`define _ARBITER_ENV_

class arbiter_env extends uvm_env;
   `uvm_component_utils(arbiter_env)
   
   arbiter_scoreboard score_trans;
   arbiter_agent agent_trans;
   
   function new(string name = "", uvm_component parent);
      super.new(name, parent);
   endfunction: new
   
   function void build_phase(uvm_phase phase);
      super.build_phase(phase);
      score_trans = arbiter_scoreboard::type_id::create(.name("score_trans"), .parent(this));
      agent_trans = arbiter_agent::type_id::create(.name("agent_trans"), .parent(this));
   endfunction: build_phase
   
   function void connect_phase(uvm_phase phase);
      super.connect_phase(phase);
      agent_trans.agent_before.connect(score_trans.monitor_before);
      agent_trans.agent_after.connect(score_trans.monitor_after);
   endfunction: connect_phase
   
endclass: arbiter_env

`endif









