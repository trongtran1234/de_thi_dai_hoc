`ifndef _ARBITER_INTERFACE_
`define _ARBITER_INTERFACE_

interface arbiter_if;
   logic ARB_RST;
   logic ARB_CLK;
   logic P0_CLK;
   logic P1_CLK;
   logic P2_CLK;
   logic P3_CLK;
   logic P0_REQ;
   logic P1_REQ;
   logic P2_REQ;
   logic P3_REQ;
   logic P0_ACK;
   logic P1_ACK;
   logic P2_ACK;
   logic P3_ACK;
endinterface: arbiter_if

`endif



