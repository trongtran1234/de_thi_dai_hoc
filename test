bs -m "HOSTGR_S HOSTGR_M HOSTGR_L HOSTGR_GUI" -os "SLES10 RHEL5" -M 400 ncverilog +nc64bit +licq \
 -uvm -sv arbiter_tb_top.sv +UVM_TESTNAME=arbiter_test \
    +ncelabargs+" -access +rw " \
    -define $1 \ 






